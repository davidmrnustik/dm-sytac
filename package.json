{
  "name": "sytac-frontend-assignment-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "prestart": "babel-node scripts/start.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node scripts/devServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src scripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --require ignore-styles --reporter spec scripts/test.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "deploy": "git add . && git commit -am 'Automatic commit message' && git push origin master"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
