    "dotenv": "^4.0.0",
    "node-polyglot": "^2.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-modal": "^3.0.3",
    "react-polyglot": "^0.2.4",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10"
  }
